{
  "name": "climate-economy-assistant",
  "version": "4.0.0",
  "private": true,
  "description": "Advanced Multi-Agent Workforce Development System for Massachusetts Climate Economy",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --watchAll=false",
    "test:e2e": "cypress run",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "type-check": "tsc --noEmit",
    "docker:build": "docker build -t cea-frontend .",
    "docker:run": "docker run -p 3000:3000 cea-frontend",
    "security:audit": "npm audit --audit-level=high",
    "supabase:types": "supabase gen types typescript --project-id zugdojmdktxalqflxbbh --schema public > types/supabase.ts"
  },
  "dependencies": {
    "@langchain/core": "^0.3.61",
    "@langchain/openai": "^0.5.15",
    "@sentry/nextjs": "^9.32.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "latest",
    "@tailwindcss/forms": "^0.5.7",
    "ai": "^2.2.35",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.19.1",
    "gpt-tokenizer": "^2.1.2",
    "langchain": "latest",
    "next": "^14.0.4",
    "node-fetch": "^2.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "redis": "latest",
    "tailwind-merge": "^3.3.1",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "latest",
    "@testing-library/react": "latest",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "cypress": "latest",
    "daisyui": "^4.4.24",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "jest": "29.5.0",
    "postcss": "^8.4.32",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^12.0.2",
    "tailwindcss": "^3.4.0"
  }
